<view class="content" style="{{'background-size:750rpx;'+('background:'+('url('+BGUrl+')')+';')}}"><view class="header"><view class="logo"><image src="{{logo}}" mode="scaleToFill"></image></view><view class="nameWapper"><view class="title">上海双人舞酒吧</view><view class="number">当晚人数：769</view></view><view class="avatar"><block wx:for="{{avatarList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item"><image src="{{item}}" mode="scaleToFill"></image></view></block></view></view><view class="textWapper"><view class="icon">Live</view><view class="text tk-acumin-pro">Current Songs</view></view><view class="bottomWapper"><view class="goodsWapper"><block wx:for="{{bottomList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['handleGoodsItem',['$0'],[[['bottomList','id',item.id]]]]]]]}}" class="goodsItem" bindtap="__e"><view class="{{['icon',item.id==goodsItemActive?'iconHover':'']}}"><image src="{{item.icon}}" mode="scaleToFill"></image></view><view class="name">{{item.name}}</view><view class="price">{{item.price}}</view></view></block></view><view class="payment"><view class="recharge">{{''+recharge+'   充值＞'}}</view><view class="btnGroup"><view data-event-opts="{{[['tap',[['handleConut',['$event']]]]]}}" class="number" bindtap="__e">{{number+" ＞"}}</view><view data-event-opts="{{[['tap',[['handleSubmit',['$event']]]]]}}" class="submit" bindtap="__e">送给他</view></view></view></view><block wx:if="{{isShowFillImage}}"><image class="fillImage" src="{{isShowFillImage?'https://atour-1300409046.cos.ap-shanghai.myqcloud.com/APNG/%E5%9B%9B%E5%8F%B6%E8%8D%89.png':''}}" mode="scaleToFill"></image></block></view>