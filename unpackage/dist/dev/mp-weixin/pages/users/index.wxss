/*每个页面公共css */
page,.content{
	height: 100%;
}
.swiper-box{
	background-color: rgba(0,0,0,.3);
	height: 440rpx;
	border-radius:0 0 100rpx 100rpx;
}
.swiper-item{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	 -webkit-box-pack: justify;
	 -webkit-justify-content: space-between;
	         justify-content: space-between;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	padding: 10rpx;
}
.swiper-item .avatar{
	width: 25%;
	height: 120rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	padding: 10rpx 0;
	position: relative;
}
.swiper-item .avatar image{
	width: 120rpx;
	height: 120rpx;
	margin: auto;
	border-radius: 50%;
}
.swiper-item .avatar .shadow{
	box-shadow: 0 0 40rpx #fa12c1;
}
.swiper-item .avatar .number{
	position: absolute;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	margin: auto;
	width: 120rpx;
	height: 120rpx;
	background-color: rgba(0,0,0,.3);
	font-size: 44rpx;
	text-align: center;
	line-height: 120rpx;
	font-weight: bolder;
	border-radius: 50%;
	z-index: 10;
	color: #fff;
}
.table{
	position: absolute;
	height: 515rpx;
	width: 100%;
	bottom: 0;
}
.table .tableLeft{
	width: 490rpx;
	height: 515rpx;
	overflow-y: auto;
	float: left;
}
.table .tableLeft .tableItem{
	width: 480rpx;
	height: 460rpx;
	border-radius: 140rpx;
	background-color: rgb(191 114 172 / 30%);
	margin-bottom: 10rpx;
	background-image: url(/pages/users/images/tableBG.png);
	background-size: 318rpx 318rpx;
	background-repeat: no-repeat;
	background-position: center;
	position: relative;
}
.table .tableLeft .tableItem .user{
	width: 120rpx;
	height: 120rpx;
	border-radius: 50%;
	position: absolute;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	margin: auto;
}
.shadow{
	box-shadow: 0 0 40rpx #fa12c1;
}
.zIndex{
	z-index: 100;
}
.table .tableLeft .tableItem .number{
	position: absolute;
	font-size: 44rpx;
	text-align: center;
	bottom: 10rpx;
	font-weight: bold;
	width: 100%;
	color: #BF72AC;
}
.table .tableRight{
	width: 260rpx;
	height: 100%;
	float: right;
	overflow-y: auto;
}
.table .tableRight::-webkit-scrollbar,.table .tableLeft::-webkit-scrollbar {
	display: none;
}
.table .tableRight .tableItem{
	width: 250rpx;
	height: 250rpx;
	background-color: rgba(56,13,134,.3);
	border-radius: 100rpx;
	margin: auto;
	margin-bottom: 10rpx;
	position: relative;
}
.table .tableRight .tableItem .seat{
	width: 120rpx;
	height: 120rpx;
	position: absolute;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	margin: auto;
}
.table .tableRight .tableItem .number{
	position: absolute;
	font-size: 36rpx;
	text-align: center;
	bottom: 5rpx;
	font-weight: bold;
	width: 100%;
	color: #BF72AC;
}
.redius{
	border-radius: 50%;
}
.textWapper{
	padding: 5px 20px;
	margin-top: 76rpx;
	width: 437rpx;
	height: 139rpx;
	/* position: absolute;
 */
	top: 182rpx;
}
.textWapper image{width: 100%;height: 100%;}
.textWapper .icon{
	height: 20px;
	padding: 10rpx 20rpx;
	text-align: center;
	line-height: 20px;
	display: inline-block;
	background-color: #5555ff;
	color: #ffe109;
	-webkit-transform: skewX(-20deg);
	        transform: skewX(-20deg);
}
.textWapper .text{
	font-size: 30px;
	color: #fff;
}
.bottles{
	width: 200rpx;
	height: 280rpx;
	position: absolute;
	top:33vh;
	right: 60rpx;
}
.bottles image{
	width: 100%;
	height: 100%;
}

